    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "superagent": "^3.6.0",
    "todomvc-app-css": "^2.0.0",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "standard": "^10.0.2",
    "webpack": "^3.5.6"
  }
