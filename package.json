    "lint": "standard \"src/**/*.jsx\"",
    "propublish": "npm run clean && npm run lint && npm run build",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "lodash": "^4.17.4",
